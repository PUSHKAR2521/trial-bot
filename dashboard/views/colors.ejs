      --red-contrast: #fff;
      --red-surface: color(display-p3 0.165 0.086 0.071 / 0.5);
      --red-indicator: oklch(59.8% 0.198 25.52);
      --red-track: oklch(59.8% 0.198 25.52);
    }
  }
}
