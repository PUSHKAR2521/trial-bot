      --red-8: oklch(54.3% 0.148 25.52);
      --red-9: oklch(59.8% 0.198 25.52);
      --red-10: oklch(55.5% 0.148 25.52);
      --red-11: oklch(78.4% 0.166 25.52);
      --red-12: oklch(90.2% 0.053 25.52);

      --red-a1: color(display-p3 0.961 0 0 / 0.022);
